{
  "version": 2,
  "builds": [
    {
      "src": "backend/main_supabase.py",
      "use": "@vercel/python"
    },
    {
      "src": "backend/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/backend/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/backend/main_supabase.py"
    },
    {
      "src": "/(.*)",
      "dest": "/backend/main_supabase.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
